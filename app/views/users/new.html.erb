<% provide(:title, 'Registrar') %> <h1>Registrar</h1>

<div class="row">
	<div class="span6 offset3">

		<%= form_for(@user) do |f| %>
		<%= render 'shared/error_messages', object: f.object %>
		<%= render 'fields', f: f %>
		<%= f.submit "Criar conta", class: "btn btn-large btn-primary", :disable_with => "Aguarde..." %>
		<% end %>
	</div>
</div>